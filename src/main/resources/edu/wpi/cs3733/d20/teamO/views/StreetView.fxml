<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.web.WebView?>
<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
  style="-fx-font-size: 16" fx:controller="edu.wpi.cs3733.d20.teamO.view_model.StreetViewViewModel">
  <WebView fx:id="webView" AnchorPane.topAnchor="0" AnchorPane.bottomAnchor="0"
    AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0"/>
  <VBox alignment="CENTER" spacing="8" AnchorPane.topAnchor="8" AnchorPane.rightAnchor="8"
    maxWidth="-Infinity" maxHeight="-Infinity"
    style="-fx-background-color: whitesmoke; -fx-background-radius: 24; -fx-padding: 12 16;">
    <JFXButton onAction="#goToFaulkner" text="%streetViewSwitchToFaulkner"/>
    <JFXButton onAction="#goToMainCampus" text="%streetViewSwitchToMainCampus"/>
  </VBox>
</AnchorPane>
