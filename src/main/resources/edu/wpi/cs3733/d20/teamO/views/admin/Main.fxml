<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import javafx.collections.FXCollections?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox alignment="CENTER" spacing="32" style="-fx-padding: 32; -fx-font-size: 16"
  xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1"
  fx:controller="edu.wpi.cs3733.d20.teamO.view_model.admin.MainAdminViewModel">
  <Label style="-fx-font-size: 36" text="%adminOptionsTitle"/>
  <JFXButton buttonType="RAISED" onAction="#openFloorMapEditor"
    style="-fx-background-color: lightgray" text="%accessFloorMapEditor"/>
  <JFXButton buttonType="RAISED" onAction="#openServiceRequestHandler"
    style="-fx-background-color: lightgray" text="%accessServiceRequestHandler"/>
  <JFXButton buttonType="RAISED" layoutX="82.0" layoutY="197.0" onAction="#openEmployeeHandler"
    style="-fx-background-color: lightgray" text="%accessEmployeeHandler"/>
  <JFXButton buttonType="RAISED" onAction="#openImportExportHandler"
    style="-fx-background-color: lightgray" text="%accessCSVIE"/>
  <HBox alignment="CENTER" spacing="16">
    <Label text="%accessAlgorithms"/>
    <JFXComboBox fx:id="searchAlgorithms">
      <items>
        <FXCollections fx:factory="observableArrayList">
          <Label text="%aStar"/>
          <Label text="%bfs"/>
          <Label text="%dfs"/>
          <Label text="%dijkstra"/>
        </FXCollections>
      </items>
    </JFXComboBox>
  </HBox>
  <HBox alignment="CENTER" spacing="16">
    <Label text="%timeout"/>
    <Spinner fx:id="timeOutTime">
    </Spinner>
  </HBox>
  <HBox alignment="CENTER_RIGHT">
    <JFXButton onAction="#close" text="%adminLogout"/>
  </HBox>
</VBox>
