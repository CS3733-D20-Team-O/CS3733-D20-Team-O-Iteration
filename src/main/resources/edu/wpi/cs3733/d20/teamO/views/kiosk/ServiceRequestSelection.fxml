<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity"
  xmlns="http://javafx.com/javafx/11.0.1"
  fx:controller="edu.wpi.cs3733.d20.teamO.view_model.kiosk.ServiceRequestSelection"
  fx:id="root">
  <VBox alignment="CENTER" fx:id="headerContainer">
    <HBox alignment="CENTER" spacing="30.0" fx:id="header">
      <Label text="OR"/>
      <VBox alignment="CENTER" spacing="20.0">
        <JFXComboBox promptText="%kioskWelcome" fx:id="serviceSelector"/>
        <Label text="%kioskWelcome"/>
      </VBox>
      <VBox alignment="CENTER" spacing="20.0">
        <HBox alignment="CENTER" spacing="10.0">
          <JFXButton text="Lookup"/>
          <TextField/>
        </HBox>
        <Label text="Label"/>
      </VBox>
    </HBox>
  </VBox>
</AnchorPane>
