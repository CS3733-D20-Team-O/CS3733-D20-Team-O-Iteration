<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import javafx.collections.FXCollections?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity"
  minHeight="600.0"
  xmlns="http://javafx.com/javafx/11.0.1"
  fx:controller="edu.wpi.cs3733.d20.teamO.view_model.kiosk.ServiceRequestSelection"
  fx:id="root">
  <HBox alignment="CENTER_RIGHT" spacing="10.0" fx:id="bottomButtons">
    <JFXButton onAction="#onCancelClicked" text="Cancel"/>
    <JFXButton onAction="#onSubmitClicked" text="Submit"/>
  </HBox>
  <VBox alignment="CENTER" fx:id="headerContainer">
    <HBox alignment="CENTER" spacing="30.0" fx:id="header">
      <Label text="OR"/>
      <VBox alignment="CENTER" spacing="20.0">
        <JFXComboBox promptText="%kioskWelcome" fx:id="serviceSelector">
          <items>
            <FXCollections fx:factory="observableArrayList">
              <Label text="%serviceGiftDeliveryDescription"/>
            </FXCollections>
          </items>
        </JFXComboBox>
        <Label text="%kioskWelcome"/>
      </VBox>
      <VBox alignment="CENTER" spacing="20.0">
        <HBox alignment="CENTER" spacing="10.0">
          <JFXButton text="Lookup"/>
          <TextField/>
        </HBox>
        <Label text="Label"/>
      </VBox>
    </HBox>
  </VBox>
</AnchorPane>
