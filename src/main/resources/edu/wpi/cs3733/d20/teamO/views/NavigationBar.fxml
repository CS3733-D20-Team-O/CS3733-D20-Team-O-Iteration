<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" minHeight="50.0" minWidth="300.0"
  xmlns="http://javafx.com/javafx/10.0.2-internal"
  fx:controller="edu.wpi.cs3733.d20.teamO.view_model.NavigationBar">
  <HBox AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" alignment="CENTER">
    <Label text="${controller.title}"/>
  </HBox>
  <JFXButton AnchorPane.leftAnchor="0" onAction="#onBackPressed" text="%backButton"/>
</AnchorPane>
