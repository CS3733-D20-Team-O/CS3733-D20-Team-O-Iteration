<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0"
    xmlns="http://javafx.com/javafx/10.0.2-internal"
    fx:controller="edu.wpi.onyx_ouroboros.view_model.prototype.ModifyDatabaseViewModel">
    <Button GridPane.halignment="CENTER" GridPane.valignment="CENTER" alignment="CENTER"
        contentDisplay="CENTER" mnemonicParsing="false" onAction="#addNodeBtn" prefHeight="44.0"
        prefWidth="150.0" text="Add Node to Database" fx:id="addNodeBtn"/>
    <Button GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER"
        alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false"
        onAction="#updateNodeBtn" prefHeight="42.0" prefWidth="148.0"
        text="Update an Existing Node" fx:id="updateNodeBtn"/>
    <Button GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.valignment="CENTER"
        alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false"
        onAction="#deleteNodeBtn" prefHeight="42.0" prefWidth="164.0"
        text="Delete Node from Database" fx:id="deleteNodeBtn"/>
    <TextField GridPane.columnIndex="4" GridPane.rowIndex="1" text="Update Node"
        fx:id="updateNodeNodeField"/>
    <TextField GridPane.columnIndex="2" GridPane.rowIndex="1" text="Update ID"
        fx:id="updateNodeIDField"/>
    <TextField GridPane.columnIndex="2" text="Add ID" fx:id="addIDField"/>
    <TextField GridPane.columnIndex="2" GridPane.rowIndex="2" text="Delete ID"
        fx:id="deleteNodeIDField"/>
    <VBox GridPane.columnIndex="6" prefHeight="200.0" prefWidth="100.0">
        <TextField text="Add Building" fx:id="addBuildingField"/>
        <TextField text="Add Node Type" fx:id="addNodeTypeField"/>
        <TextField text="Add Long Name" fx:id="addLongNameField"/>
        <TextField text="Add Short Name" fx:id="addShortNameField"/>
        <padding>
            <Insets top="18.0"/>
        </padding>
    </VBox>
    <VBox GridPane.columnIndex="4">
        <TextField text="Add X-Coord" fx:id="addXCoordField"/>
        <TextField text="Add Y-Coord" fx:id="addYCoordField"/>
        <TextField text="Add Floor" fx:id="addFloorField"/>
        <padding>
            <Insets top="30.0"/>
        </padding>
    </VBox>
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="223.0" minWidth="10.0" prefWidth="188.0"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="223.0" minWidth="10.0" prefWidth="22.0"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="294.0" minWidth="10.0" prefWidth="121.0"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="294.0" minWidth="9.0" prefWidth="9.0"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="294.0" minWidth="10.0" prefWidth="122.0"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="294.0" minWidth="9.0" prefWidth="9.0"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="294.0" minWidth="10.0" prefWidth="132.0"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="294.0" minWidth="10.0" prefWidth="30.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    </rowConstraints>
</GridPane>
